{
    "name": "vscode-cfengine",
    "displayName": "vscode-cfengine",
    "description": "CFEngine language bindings",
    "version": "0.0.1",
    "publisher": "azaugg",
	"repository": {
		"type": "git",
		"url": "https://github.com/AZaugg/vscode-cfengine"
	},    
    "engines": {
        "vscode": "^1.12.0"
    },
    "categories": [
        "Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "cfengine",
            "aliases": ["CFEngine", "cfengine"],
            "extensions": [".cf",".cf3"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "cfengine",
            "scopeName": "source.cfengine",
            "path": "./syntaxes/cfe"
        }]
    }
}
